{
  "name": "@serlo/ece-as-a-service",
  "version": "0.0.0",
  "private": true,
  "bugs": {
    "url": "https://github.com/serlo/ece-as-a-service/issues"
  },
  "repository": "serlo/ece-as-a-service",
  "license": "Apache-2.0",
  "author": "Serlo Education e.V.",
  "type": "module",
  "scripts": {
    "_eslint": "eslint \"{src/**/*,*}.{js,jsx,ts,tsx}\"",
    "_prettier": "prettier .",
    "build": "next build",
    "dev": "yarn start:server",
    "docker:build": "docker build -t ece-as-a-service-local-build:latest . && docker image prune --force --filter label=stage=build",
    "docker:run": "docker run --add-host=host.docker.internal:host-gateway --env-file .env --env-file .env-docker -p 3000:3000 --detach --name ece-as-a-service-from-local-build ece-as-a-service-local-build",
    "docker:stop": "docker container stop ece-as-a-service-from-local-build && docker container rm ece-as-a-service-from-local-build",
    "docker:rm": "docker image rm ece-as-a-service-local-build",
    "e2e": "cypress run",
    "e2e:with-server": "start-server-and-test start:server '3000' e2e",
    "e2e:cypress": "cypress open",
    "e2e:with-docker": "npm-run-all -c docker:build start:mongodb docker:run e2e docker:stop docker:rm",
    "format": "run-p -c \"format:*\"",
    "format:eslint": "yarn _eslint --fix",
    "format:prettier": "yarn _prettier --write",
    "lint": "run-p \"lint:*\"",
    "lint:eslint": "yarn _eslint",
    "lint:prettier": "yarn _prettier --check",
    "lint:tsc": "tsc --noEmit",
    "start:all": "npm-run-all start:mongodb -p -l start:edusharing start:server",
    "start:all-docker": "npm-run-all start:mongodb docker:run start:edusharing",
    "start:edusharing": "NODE_OPTIONS='--experimental-specifier-resolution=node' nodemon --watch edusharing-server.ts --watch src/server-utils.ts --watch src/edusharing-server-mock.ts edusharing-server.ts",
    "start:in-docker-container": "NODE_ENV=production node --experimental-modules --experimental-specifier-resolution=node server.js",
    "start:mongodb": "docker-compose up -d",
    "start:server": "TS_NODE_PROJECT='tsconfig.server.json' NODE_OPTIONS='--experimental-specifier-resolution=node' nodemon --watch server.ts --watch src server.ts",
    "stop:mongodb": "docker-compose down",
    "test": "node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:with-server": "start-server-and-test start:server '3000' test"
  },
  "resolutions": {
    "@fortawesome/fontawesome-svg-core": "^6.1.2",
    "@fortawesome/free-solid-svg-icons": "^6.1.2",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@types/react": "^18.0.0",
    "@types/react-dom": "^18.0.0"
  },
