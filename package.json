    "@edtr-io/plugin-multimedia-explanation": "^2.8.2",
    "@edtr-io/plugin-rows": "^2.8.2",
    "@edtr-io/plugin-sc-mc-exercise": "^2.8.2",
    "@edtr-io/plugin-serlo-injection": "^2.8.3",
    "@edtr-io/plugin-spoiler": "^2.8.2",
    "@edtr-io/plugin-text": "^2.8.2",
    "@edtr-io/renderer": "^2.8.2",
    "@edtr-io/store": "^2.8.2",
    "@edtr-io/ui": "^2.5.0",
    "@fortawesome/fontawesome-svg-core": "^6.2.1",
    "@fortawesome/free-solid-svg-icons": "^6.2.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "@next/env": "^13.1.1",
    "body-parser": "^1.20.1",
    "clsx": "^1.2.1",
    "express": "^4.18.2",
    "form-data-encoder": "^2.1.4",
    "formdata-node": "^5.0.0",
    "immutable": "^4.2.1",
    "json-web-key": "^0.4.0",
    "jsonwebtoken": "^9.0.0",
    "jwks-rsa": "^3.0.0",
    "ltijs": "^5.9.0",
    "next": "^13.1.1",
    "node-fetch": "^3.3.0",
    "raw-body": "^2.5.1",
    "react": "^18.2.0",
    "react-beautiful-dnd": "^13.1.1",
    "react-dnd": "^15.1.2",
    "react-dnd-html5-backend": "^15.1.3",
